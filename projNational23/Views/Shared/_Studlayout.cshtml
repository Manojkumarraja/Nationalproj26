@{
        ViewBag.Title = "_Studlayout";
    }

    <!DOCTYPE html>
    <html>
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
        @RenderSection("scripts")

        <style>
            .glyphicon-user {
                position: relative;
                top: 5px;
                left: 5px;
                image-rendering: pixelated;
            }

            .row {
                text-align: center;
            }

            button1 {
                background-color: green;
                border: black;
                color: black;
                position: relative;
                padding: 5px !important;
                padding-left: 8px !important;
                padding-right: 8px !important;
                text-align: center;
                text-decoration: none;
                font-size: 10px;
                cursor: pointer;
            }

            .button1 {
                border-radius: 12px;
            }

            * {
                box-sizing: border-box;
            }

            /* Create two unequal columns that floats next to each other */
            .column {
                float: left;
                padding: 10px;
                height: 300px; /* Should be removed. Only for demonstration */
            }

            .left {
                width: 15%;
            }

            .right {
                width: 85%;
            }
        </style>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
        <link rel="stylesheet" href="https://www.w3schools.com/lib/w3-theme-black.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
    </head>
    <body>
        <div class="container-fluid">
            
   <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
        </div>
        <div class="w3-top">
            <div class="w3-bar w3-theme-d2 w3-left-align">
                <a class="w3-bar-item w3-button w3-hide-medium w3-hide-large w3-right w3-hover-white w3-theme-d2" href="javascript:void(0);" onclick="openNav()"><i class="fa fa-bars"></i></a>
                @Html.ActionLink("EDU-ASSIST", "Index", "Home", null, new { @class = "w3-bar-item w3-button" })

                @Html.ActionLink("About Us", "About", "Home", null, new { @class = "w3-bar-item w3-button" })

                <a href="#contact" class="w3-bar-item w3-button w3-hide-small w3-hover-white">Contact</a>
                <div class="w3-dropdown-hover w3-hide-small">
                    <button class="w3-button" title="Notifications">Schemes <i class="fa fa-caret-down"></i></button>
                    <div class="w3-dropdown-content w3-card-4 w3-bar-block">
                        @Html.ActionLink("Scheme 1", "", null, new { @class = "w3-bar-item w3-button" })
                        @Html.ActionLink("Scheme 2", "", null, new { @class = "w3-bar-item w3-button" })
                        @Html.ActionLink("Scheme 3", "", null, new { @class = "w3-bar-item w3-button" })
                    </div>
                </div>
        @Html.Partial("_loginPartial")
        <a href="#" class="w3-bar-item w3-button w3-hide-small w3-right w3-hover-teal" title="Search"><i class="fa fa-search"></i></a>
        </div>

        <!-- Navbar on small screens -->
        <div id="navDemo" class="w3-bar-block w3-theme-d2 w3-hide w3-hide-large w3-hide-medium">
                        @Html.ActionLink("About Us", "About","Home", null, new { @class = "w3-bar-item w3-button" })
                        @Html.ActionLink("Contact", "#contact", null, new { @class = "w3-bar-item w3-button" })
                        @Html.ActionLink("Login", "Login", "Login", null, new { @class = "w3-bar-item w3-button" })
                        @Html.ActionLink("Register", "Register", "Register", null, new { @class = "w3-bar-item w3-button" })


                    </div>
                </div>

            </nav>
        </div>

        <h3>Registration</h3>

        <div class="row">
            <div class="column left" style="background-color:deepskyblue;height:700px">
                <p> <span class="glyphicon glyphicon-user"></span></p>
                <div class="btn-group" style="width:100%">

                    <br />
                    @*<button style="width:150px;font-size:15px">@Html.ActionLink("Renewal", "_Renewal", "Applicant")</button>*@
                </div>
                <h2>Student</h2>
                <button style="width:150px;font-size:15px">
                    @Html.ActionLink("View Status", "_ViewStatus", "Viewstat")
                </button>
            </div>
            <div class="column right" style="background-color:bisque;overflow-y:auto;height:700px;">


                <div class="btn-group" style="width:100%">
                    <button style="width:150px;font-size:15px">@Html.ActionLink("Personaldetails", "_Applicantd", "Applicant")</button>
                    <button style="width:150px;font-size:15px">@Html.ActionLink("Academicdetails", "_Academicd", "Academic") </button>
                    <button style="width:150px;font-size:15px">@Html.ActionLink("HSC", "_AcademicHSCd", "AcademicHSC")</button>
                    <button style="width:150px;font-size:15px">@Html.ActionLink("SSC", "_AcademicSSCd", "AcademicSSC")</button>
                    <button style="width:150px;font-size:15px">@Html.ActionLink("National Education", "_NationalEdud", "National_Edu")</button>
                    <button style="width:150px;font-size:15px">@Html.ActionLink("Bank Details", "_Regschemed", "Regscheme")</button>
                    <button style="width:150px;font-size:15px">@Html.ActionLink("Upload Files", "_Files3d", "Files3")</button>

                </div>
                @RenderBody();
            </div>
        </div>
    </body>
    </html>
    <meta name="viewport" content="width=device-width, initial-scale=1">


